{
  "name": "@fsms/angular-pubsub-app",
  "version": "1.0.19",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:prod": "ng build fsms-angular-pubsub --prod",
    "test": "ng test fsms-angular-pubsub",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rimraf dist",
    "build:ci": "npm run clean && npm run build:prod && npm run copyfiles",
    "test:ci": "ng test fsms-angular-pubsub --watch=false --browsers=ChromeHeadless --code-coverage=true",
    "copyfiles": "copyfiles -f README.md projects/fsms-angular-pubsub/.npmrc dist/fsms-angular-pubsub",
    "release:git": "standard-version --release-as patch && git add . package.json && git commit -m \"chore: bump version number\" && git push --follow-tags origin main",
    "release:npm": "npm run build:ci && npm publish dist/fsms-angular-pubsub --access public",
    "release": "npm run release:git && npm run release:npm",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.4.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^2.0.1",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "standard-version": "^9.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
